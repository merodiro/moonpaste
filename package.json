{
  "name": "root",
  "version": "0.0.0",
  "scripts": {
    "dev": "turbo run dev"
  },
  "dependencies": {
    "turbo": "^1.0.29"
  },
  "workspaces": [
    "apps/*"
  ],
  "turbo": {
    "pipeline": {
      "dev": {
        "cache": false
      },
      "build": {
        "outputs": [
          "dist/**",
          ".next/**"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
